<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="All Tests">

    <listeners>
        <listener class-name="general.listeners.RetryListener"/>
    </listeners>

    <test name="test">
        <groups>
            <run>
                <include name="regression"/>
            </run>
        </groups>
        <classes>
            <class name="LoginTest"/>
            <class name="UpdateZipCodeTest"/>
        </classes>
    </test>

</suite>